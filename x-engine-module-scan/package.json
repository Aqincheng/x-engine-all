{
  "name": "@zkty-team/x-engine-module-scan",
  "version": "0.0.1",
  "main": "h5/index.js",
  "license": "MIT",
  "scripts": {
    "x-cli:autolink": "x-cli module autolink",
    "deprecated:autolink": "node scripts/autolink.js",
    "dev": "WEBPACK_MODE=development webpack-dev-server --mode development -p --open --host 0.0.0.0 --config webpack.dev.js",
    "h5": "http://www.responsinator.com/?url=localhost%3A8081",
    "build": "webpack  --config webpack.prod.js",
    "prod": "WEBPACK_MODE=production webpack  --config webpack.prod.js",
    "patch": "npm version patch",
    "test": "WEBPACK_MODE=development rm -rdf tmp &&  testem",
    "ci": "WEBPACK_MODE=development rm -rdf tmp &&testem ci",
    "perf": "WEBPACK_MODE=development rm -rdf tmp && webpack --config webpack.perf.js && node tmp/perf/main.js",
    "dev:device": "rm -rdf dist && WEBPACK_MODE=development webpack  --mode development  && cp -r dist/* ../iOS/scan/com.*.*.*/",
    "prod:device": "rm -rdf dist && WEBPACK_MODE=production webpack --mode production   --config webpack.prod.js && cp -r dist/* ../iOS/scan/com.*.*.*/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/zkty-team/x-engine.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "babel-loader": "^8.1.0",
    "benchmark": "^2.1.4",
    "css-loader": "^5.0.0",
    "file-loader": "^6.1.1",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.5.0",
    "mocha": "^8.1.3",
    "style-loader": "^2.0.0",
    "vconsole-webpack-plugin": "^1.5.2",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@zkty-team/x-engine-module-camera": "0.0.11",
    "@zkty-team/x-engine-module-engine": "~0.0.19",
    "@zkty-team/x-engine-module-microapp": "~0.0.11",
    "@zkty-team/x-engine-module-protocols": "0.0.11",
    "@zkty-team/x-engine-module-router": "0.0.11",
    "@zkty-team/x-engine-module-tools": "~0.0.14",
    "@zkty-team/x-engine-module-ui": "~0.0.23",
    "webpack-dev-server-qr-code": "^1.2.0"
  }
}
